    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "drizzle-orm": "^0.33.0",
    "express": "^4.21.0",
    "morgan": "^1.10.0",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "socket.io": "^4.7.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@dsabattle/tsconfig": "workspace:*",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
